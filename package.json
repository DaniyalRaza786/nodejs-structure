{
  "name": "nodeapp",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "nodemon client.js",
    "dev:local": "npx env-cmd -f .env.local nodemon server",
    "dev:staging": "npx env-cmd -f .env.staging nodemon server",
    "dev:production": "npx env-cmd -f .env.production nodemon server",
    "db:create": "npx sequelize-cli db:create",
    "db:down": "npx sequelize-cli db:migrate:undo",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:local": "cross-env NODE_ENV=local npx sequelize-cli db:migrate",
    "db:migrate:staging": "cross-env NODE_ENV=staging npx sequelize-cli db:migrate",
    "db:migrate:production": "cross-env NODE_ENV=production npx sequelize-cli db:migrate",
    "db:makemigrations": "node ./node_modules/sequelize-auto-migrations-v2/bin/makemigration.js",
    "db:create:seed": "npx sequelize-cli seed:generate --name your-seeder-name",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seedAll:local": "cross-env NODE_ENV=local npx sequelize-cli db:seed:all",
    "db:seedAll:staging": "cross-env NODE_ENV=staging npx sequelize-cli db:seed:all",
    "db:seedAll:production": "cross-env NODE_ENV=production npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo",
    "db:seedUndo:local": "cross-env NODE_ENV=local npx sequelize-cli db:seed:undo:all",
    "db:seedUndo:staging": "cross-env NODE_ENV=staging npx sequelize-cli db:seed:undo:all",
    "db:seedUndo:production": "cross-env NODE_ENV=production npx sequelize-cli db:seed:undo:all",
    "db:drop": "npx sequelize-cli db:drop"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "firebase": "^11.6.1",
    "firebase-admin": "^13.2.0",
    "jose": "^6.0.10",
    "jsbarcode": "^3.12.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.1",
    "nodemon": "^3.1.9",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-auto-migrations-v2": "^1.2.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  }
}
